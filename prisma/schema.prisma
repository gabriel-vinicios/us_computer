// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Members {
  id          String    @id   @default(uuid())
  name        String
  role        String    
  created_at  DateTime  @default(now())
  description String

  @@map("members")
}